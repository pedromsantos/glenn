{
  "name": "glenn",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-typescript": "^7.15.0",
    "@fluffy-spoon/substitute": "^1.208.0",
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "fast-check": "^2.18.0",
    "husky": "^7.0.4",
    "jest": "^27.4.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "watch": "react-scripts test",
    "test": "react-scripts test --watchAll=false --coverage=true",
    "test:mutants": "stryker run",
    "eject": "react-scripts eject",
    "lint": "eslint --color --ext .html,.js,.json,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --fix --ext .html,.js,.json,.jsx,.ts,.tsx src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/__test__/utils.ts",
      "!src/react-app-env.d.ts",
      "!src/App.tsx",
      "!src/index.tsx"
    ],
    "coverageReporters": [
      [
        "text"
      ]
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  }
}
