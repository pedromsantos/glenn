{
  "name": "glenn",
  "version": "1.0.0",
  "description": "Module for working with music theory",
  "author": "Pedro Moreira Santos",
  "homepage": "https://github.com/pedromsantos/glenn",
  "keywords": [
    "Music",
    "Theory",
    "Music Theory",
    "Guitar",
    "Tab",
    "Tablature"
  ],
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "watch": "jest --watch",
    "clean": "rm -r ./lib",
    "test": "jest --watchAll=false",
    "precommit": "npm-run-all -p clean lint types test:ci build",
    "test:ci": "jest test --watchAll=false --coverage=true",
    "mutants": "stryker run",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "lint": "eslint . --color --ext .ts --ignore-pattern 'node_modules/'",
    "lint:fix": "eslint . --ext .ts --ignore-pattern 'node_modules/' --fix",
    "types": "type-coverage -at-least 99",
    "types-report": "typescript-coverage-report --threshold 99",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@stryker-mutator/core": "^6.3.0",
    "@stryker-mutator/jest-runner": "^6.3.0",
    "@stryker-mutator/typescript-checker": "^6.3.0",
    "@types/jest": "^28.1.7",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "fast-check": "^3.3.0",
    "husky": "^8.0.2",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-coverage": "^0.1.5",
    "ts-jest": "^28.0.8",
    "type-coverage": "^2.23.0",
    "typescript": "^4.8.4",
    "typescript-coverage-report": "^0.6.4"
  }
}
