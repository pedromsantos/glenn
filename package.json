{
  "name": "glenn",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@fluffy-spoon/substitute": "^1.208.0",
    "@stryker-mutator/core": "^5.6.0",
    "@stryker-mutator/jest-runner": "^5.6.0",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/jest": "^27.0.2",
    "@types/react-helmet": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "fast-check": "^2.18.0",
    "prettier": "^2.4.1",
    "ts-jest": "^26.4.2",
    "ts-node-dev": "^1.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "watch": "react-scripts test",
    "test": "react-scripts test --watchAll=false --coverage=true",
    "test:mutants": "stryker run",
    "eject": "react-scripts eject",
    "eslint": "eslint --color --ext .html,.js,.json,.jsx,.ts,.tsx src",
    "eslint:fix": "eslint -- --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/__test__/utils.ts",
      "!src/react-app-env.d.ts",
      "!src/App.tsx",
      "!src/index.tsx"
    ],
    "coverageReporters": [
      [
        "text"
      ]
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
