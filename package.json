{
  "name": "glenn",
  "version": "0.0.0",
  "description": "Module for working with music theory",
  "author": "Pedro Moreira Santos",
  "homepage": "https://github.com/pedromsantos/glenn",
  "keywords": [
    "Music",
    "Theory",
    "Music Theory",
    "Guitar",
    "Tab",
    "Tablature"
  ],
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "release": "standard-version",
    "watch": "jest --watch",
    "clean": "rm -r ./lib",
    "test": "jest --watchAll=false",
    "precommit": "npm-run-all -p clean lint types test:ci build",
    "test:ci": "jest test --watchAll=false --coverage=true",
    "mutants": "stryker run",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "lint": "eslint . --color --ext .ts --ignore-pattern 'node_modules/'",
    "lint:fix": "eslint . --ext .ts --ignore-pattern 'node_modules/' --fix",
    "types": "type-coverage -at-least 99",
    "types-report": "typescript-coverage-report --threshold 99",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@stryker-mutator/core": "^6.3.0",
    "@stryker-mutator/jest-runner": "^6.3.0",
    "@stryker-mutator/typescript-checker": "^6.3.0",
    "@types/jest": "^28.1.7",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "fast-check": "^3.3.0",
    "husky": "^8.0.2",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "standard-version": "^9.5.0",
    "ts-coverage": "^0.1.5",
    "ts-jest": "^28.0.8",
    "type-coverage": "^2.24.1",
    "typescript": "^4.9.3",
    "typescript-coverage-report": "^0.7.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case"
        ]
      ]
    }
  }
}
