{
  "collectCoverageFrom": ["src/**/*.ts", "!lib/**/*.*", "!src/__test__/utils.ts", "!src/index.ts"],
  "coverageReporters": ["json", "lcov", "text", "html"],
  "coverageThreshold": {
    "global": {
      "branches": 85,
      "functions": 95,
      "lines": 95,
      "statements": 95
    }
  },
  "preset": "ts-jest",
  "transform": {
    "^.+\\.ts?$": "ts-jest",
    "^.+\\.js$": "babel-jest"
  }
}
