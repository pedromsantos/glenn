{
  "collectCoverageFrom": ["src/**/*.ts", "!lib/**/*", "!src/__test__/**/*", "!src/index.ts"],
  "coverageReporters": ["json", "lcov", "text", "html"],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 95,
      "lines": 98,
      "statements": 98
    }
  },
  "preset": "ts-jest"
}
